{
	"devDependencies": {
		"@types/node": "^18.0.1",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"eslint": "^8.32.0",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.4"
	},
	"scripts": {
		"prepare": "node -v | grep v18 || (echo 'Wrong Node version' && exit 1); [ -d .git ] && (P=.git/hooks/pre-commit; echo \"#!/bin/sh\\nnpm run lint:check\" > $P; chmod 777 $P)",
		"dev": "ts-node-dev --quiet --clear --rs --respawn src/app.ts",
		"build": "tsc",
		"lint": "eslint src/",
		"lint:check": "eslint --max-warnings 0 src",
		"lint:fix": "eslint --fix src",
		"start": "node build/app.js"
	}
}
